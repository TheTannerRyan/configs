    },
    "cSpell.language": "en-GB",
    "breadcrumbs.enabled": true,
    "editor.formatOnSave": true,
    "latex-workshop.latex.autoBuild.onSave.enabled": true,
    "workbench.colorTheme": "Tomorrow",
}

